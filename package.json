{
  "name": "anysols-model",
  "version": "0.6.0",
  "description": "Mongoose wrapper with intercepts",
  "main": "./index.js",
  "scripts": {
    "test": "nyc mocha --recursive",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "check-coverage": false,
    "all": true,
    "extension": [
      ".js"
    ],
    "include": [
      "**/!(*.spec.*).js"
    ],
    "exclude": [
      "docs/",
      "**/gulpfile.js",
      "test/",
      "playground.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "docs/reports/coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anysols/anysols-model.git"
  },
  "author": "Shivaji Varma <contact@shivajivarma.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://gitlab.com/anysols/anysols-model/issues"
  },
  "homepage": "https://anysols.com",
  "dependencies": {
    "lodash": "^4.17.10",
    "mongoose": "^5.2.6"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0"
  }
}
